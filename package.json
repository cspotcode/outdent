  "author": "Andrew Bradley <cspotcode@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cspotcode/outdent/issues"
  },
  "homepage": "https://github.com/cspotcode/outdent#readme",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.44",
    "@types/rimraf": "^2.0.2",
    "@types/source-map-support": "^0.4.0",
    "@types/which": "^1.0.28",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.5.3",
    "which": "^1.3.0"
  },
  "files": [
    "lib",
    "lib-module",
    "src",
    "LICENSE",
    "README.md",
    "tsconfig-module.json",
    "tsconfig.json"
  ]
}
